<div class="page-header">
  <h3><%= @user.username %>'s Liked Photos'</h3>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Caption</th>
        <th>Image</th>
      </tr>

      <% @user.liked_photos.each do |photo| %>
      <tr>
        <td><%= photo.caption %></td>
        <td><img src="<%= photo.image %>"></td>
        <td>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
